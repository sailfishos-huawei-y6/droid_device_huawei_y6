
#mount point      fstype       	device         											 [device2] 				[length=]

/system		ext4	/dev/block/mmcblk0p24		flags=backup=1
/cache		ext4	/dev/block/mmcblk0p22		flags=backup=1
/data		ext4	/dev/block/mmcblk0p25		flags=backup=1		length=-16384
/external_sd	vfat	/dev/block/mmcblk1p1		flags=display="Micro SDcard";storage;removable
/boot		emmc	/dev/block/mmcblk0p19		flags=backup=1
/recovery	emmc	/dev/block/mmcblk0p20		flags=backup=1
/misc		emmc	/dev/block/mmcblk0p10

